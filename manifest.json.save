{
  "manifest_version": 2,
  "name": "Url shortcuts",
  "description": "Shortcuts for urls",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background" : {
    "scripts": ["background.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],

}
